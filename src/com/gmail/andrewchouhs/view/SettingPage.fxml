<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="513.0" prefWidth="607.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.gmail.andrewchouhs.controller.SettingPageController">
  <!-- TODO Add Nodes -->
  <children>
    <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <tabs>
        <Tab text="音樂管理">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="anchor-pane">
              <children>
                <TableView fx:id="dirInfoTable" prefHeight="-1.0" prefWidth="-1.0" tableMenuButtonVisible="false" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
                  <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
                  <columns>
                    <TableColumn maxWidth="5000.0" minWidth="0.0" prefWidth="565.0" text="路徑" fx:id="pathColumn" />
                  </columns>
                </TableView>
                <HBox prefHeight="-1.0" prefWidth="-1.0" spacing="10.0" AnchorPane.bottomAnchor="8.0" AnchorPane.rightAnchor="20.0">
                  <children>
                    <Button mnemonicParsing="false" onMouseClicked="#addDir" text="選擇資料夾" />
                    <Button mnemonicParsing="false" onMouseClicked="#removeDir" text="刪除" />
                    <Button mnemonicParsing="false" onMouseClicked="#removeAllDir" text="全部刪除" />
                    <Button mnemonicParsing="false" onMouseClicked="#removeChildrenDir" text="子資料夾一併刪除" />
                    <Button mnemonicParsing="false" onMouseClicked="#okDir" text="確定" />
                    <Button mnemonicParsing="false" onMouseClicked="#cancelDir" text="取消" />
                  </children>
                </HBox>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab text="偏好設定">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="anchor-pane">
              <children>
                <VBox prefHeight="-1.0" prefWidth="-1.0" spacing="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0">
                  <children>
                    <CheckBox alignment="CENTER_LEFT" contentDisplay="LEFT" graphicTextGap="4.0" mnemonicParsing="false" selected="false" text="開機啟動程式" />
                    <CheckBox mnemonicParsing="false" text="開啟程式時自動播放" />
                    <CheckBox mnemonicParsing="false" text="自動更新" />
                    <CheckBox mnemonicParsing="false" text="提醒更新" />
                  </children>
                </VBox>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab text="更新">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="anchor-pane" />
          </content>
        </Tab>
        <Tab text="關於">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="anchor-pane" />
          </content>
        </Tab>
      </tabs>
    </TabPane>
  </children>
  <stylesheets>
    <URL value="@SettingPage.css" />
  </stylesheets>
</AnchorPane>
